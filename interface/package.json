{
  "name": "interface",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@cfxjs/use-wallet": "^0.0.26",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.13",
    "bignumber.js": "^9.0.1",
    "clsx": "^1.1.1",
    "ethers": "^5.6.4",
    "i18next": "^21.6.11",
    "js-conflux-sdk": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "react-use": "^17.3.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "cross-env TAILWIND_MODE=watch REACT_APP_TestNet=true craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write",
      "stylelint"
    ]
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": false,
    "proseWrap": "never",
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.2",
    "autoprefixer": "^9",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.10",
    "postcss-px-to-viewport": "^1.1.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "webpack-dev-server": "^4.4.0"
  }
}
